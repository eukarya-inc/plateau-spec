                "33.3333%",
                "66.6667%"
              ]
            },
            "content": [
              {
                "content": [
                  {
                    "attrs": {
                      "background": null,
                      "colspan": null,
                      "colwidth": null,
                      "rowspan": null
                    },
                    "content": [
                      {
                        "attrs": {
                          "resourceID": "urn:metanorma:doc-part:_8e94d265-318b-5593-bfe7-c8eb6bd78013"
                        },
                        "content": [
                          {
                            "text": "notificationNumber、custodian",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ],
                    "type": "table_cell"
                  },
                  {
                    "attrs": {
                      "background": null,
                      "colspan": null,
                      "colwidth": null,
                      "rowspan": null
                    },
                    "content": [
                      {
                        "attrs": {
                          "resourceID": "urn:metanorma:doc-part:_ba2f66e6-e559-69d1-be7a-521526d04a6e"
                        },
                        "content": [
                          {
                            "text": "不明な場合は「Null」とする。",
                            "type": "text"
                          }
                        ],
                        "type": "paragraph"
                      }
                    ],
                    "type": "table_cell"
                  }
                ],
                "type": "table_row"
